=== tile ===

type: screen
background: {background}

children:
  - type: group
    width: 100%
    height: 50%
    children:
      - type: image
        alt: title image
        url: images/title.jpg

  - type: input-text
    field: username
    label: Username

  - type: input-password
    field: password
    label: Password



